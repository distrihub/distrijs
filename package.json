{
  "name": "@distri/workspace",
  "version": "0.1.2",
  "private": true,
  "workspaces": [
    "distri/*"
  ],
  "exports": {
    "./core": {
      "types": "./distri/core/dist/index.d.ts",
      "import": "./distri/core/dist/index.mjs",
      "require": "./distri/core/dist/index.js"
    },
    "./react": {
      "types": "./distri/react/dist/index.d.ts",
      "import": "./distri/react/dist/index.mjs",
      "require": "./distri/react/dist/index.js"
    }
  },
  "files": [
    "distri/core/dist/",
    "distri/react/dist/",
    "README.md"
  ],
  "scripts": {
    "build": "turbo build --filter=./distri/*",
    "build:all": "turbo build",
    "build:prepare": "npm run build:all && npm run build:copy",
    "build:copy": "node scripts/build-copy.js",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  },
  "dependencies": {
    "@a2a-js/sdk": "https://github.com/v3g42/a2a-js.git#51444c9",
    "lucide-react": "^0.460.0"
  }
}