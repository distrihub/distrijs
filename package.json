{
  "name": "@distri",
  "version": "0.2.4",
  "private": true,
  "packageManager": "pnpm@10.11.1",
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    ".": {
      "types": "./packages/core/dist/index.d.ts",
      "import": "./packages/core/dist/index.mjs",
      "require": "./packages/core/dist/index.js"
    },
    "./react": {
      "types": "./packages/react/dist/index.d.ts",
      "import": "./packages/react/dist/index.js",
      "require": "./packages/react/dist/index.cjs"
    },
    "./react/globals.css": "./packages/react/dist/globals.css",
    "./components": {
      "types": "./packages/components/dist/index.d.ts",
      "import": "./packages/components/dist/index.mjs",
      "require": "./packages/components/dist/index.js"
    },
    "./fs": {
      "types": "./packages/fs/dist/index.d.ts",
      "import": "./packages/fs/dist/index.js",
      "require": "./packages/fs/dist/index.cjs"
    }
  },
  "files": [
    "packages/core/dist/",
    "packages/fs/dist/",
    "packages/react/dist/",
    "packages/components/dist/",
    "README.md"
  ],
  "scripts": {
    "build": "turbo build --filter=./packages/*",
    "build:all": "turbo build",
    "build:prepare": "npm run build:all && npm run build:copy",
    "build:copy": "node scripts/build-copy.js",
    "dev": "turbo dev",
    "dev:maps": "turbo dev --filter=./packages/react --filter=./packages/core --filter=./samples/maps-demo",
    "dev:file": "turbo dev --filter=./packages/react --filter=./packages/core --filter=./samples/file-tools-demo",
    "dev:distri": "turbo dev --filter=./packages/core --filter=./packages/react  --filter=./packages/fs --filter=./apps/distri-ui",
    "dev:tauri": "pnpm --filter \"./distri-ui\" dev:tauri",
    "dev:bot": "turbo dev --filter=./packages/react --filter=./packages/core --filter=./apps/botfleet-ui",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "clean": "turbo clean",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "turbo": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@a2a-js/sdk": "https://github.com/v3g42/a2a-js.git#51444c9",
    "lucide-react": "^0.460.0"
  }
}