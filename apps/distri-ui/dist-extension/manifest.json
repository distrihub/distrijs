{
  "manifest_version": 3,
  "name": "Distri Extension",
  "version": "0.1.1",
  "description": "Distri UI as a Chrome Extension",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "index-extension.html",
    "default_title": "Distri Extension"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-cWxssGRE.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/content.ts-cWxssGRE.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
