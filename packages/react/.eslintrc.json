{
  "extends": ["../../.eslintrc.js"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["error", { 
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_" 
    }],
    "no-case-declarations": "off"
  },
  "ignorePatterns": [
    "dist/",
    "node_modules/",
    "src/utils/test*.ts",
    "src/utils/controlTags.ts",
    "*.config.*"
  ]
}